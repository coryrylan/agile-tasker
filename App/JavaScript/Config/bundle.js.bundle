    <file>/App/JavaScript/Src/controllers.js</file>
    <file>/App/JavaScript/Src/directives.js</file>
    <file>/App/JavaScript/Src/agile-history.js</file>
    <file>/App/JavaScript/Src/agile-clock.js</file>
    <file>/App/JavaScript/Src/filters.js</file>
    <file>/App/JavaScript/Src/services.js</file>
    <file>/App/JavaScript/Src/notificationService.js</file>
