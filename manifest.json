{
  "name": "Agile Tasker",
  "description": "A small task timer to help agile thinking methodologies.",
  "manifest_version": 2,
  "version": "1.0.2",
  "app": {
    "urls": [
      "*://splintercode.github.io/AgileTasker/"
    ],
    "launch": {
      "web_url": "http://splintercode.github.io/AgileTasker/"
    }
  },
  "icons": {
    "128": "Content/icon.png"
  },
  "permissions": [
    "background",
    "notifications"
  ]
}