{
  "name": "Agile Tasker",
  "description": "A small task timer to help agile thinking methodologies.",
  "manifest_version": 2,
  "version": "1.0.1",
  "browser_action": {
    "default_icon": "Content/logo.png",
    "default_popup":"index.html",
	"width":"300",
	"height":"300"
  },
  "icons": {
    "128": "Content/logo.png"
  },
  "permissions": [
    "background",
    "notifications"
  ]
}
